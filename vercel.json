{
  "rewrites": [
    {
      "source": "/auth/(.*)",
      "destination": "/api/app"
    },
    {
      "source": "/twitch/(.*)",
      "destination": "/api/app"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/app"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}