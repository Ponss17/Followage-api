{
  "rewrites": [
    {
      "source": "/twitch/clips",
      "destination": "/twitch/clips/index.html"
    },
    {
      "source": "/twitch/clips/(.*)",
      "destination": "/twitch/clips/$1"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/app"
    },
    {
      "source": "/twitch/(.*)",
      "destination": "/api/app"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/app"
    }
  ]
}